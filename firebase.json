{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "apps/firestore/firestore.rules",
    "indexes": "apps/firestore/firestore.indexes.json"
  },
  "functions": [
    {
      "source": "apps/functions/isolate",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\"/.. run lint",
        "npm --prefix \"$RESOURCE_DIR\"/.. run build",
        "npx isolate"
      ]
    }
  ],
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "pubsub": {
      "port": 8085
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "extensions": {}
}
